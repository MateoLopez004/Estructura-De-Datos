#include <iostream>
using namespace std;
int fibonacci(int n);
int n;
int x;
int main() {
	cout << "Ingrese la posicion N que quiere averiguar: ";
	cin >> n;
	cout << "El Valor es: " << fibonacci(n) << endl;
}
int fibonacci(int n) {
	if (n <= 0) {
		return 1;
	}
	else {
		return fibonacci(n - 1) + fibonacci(n - 2);
	}
}
